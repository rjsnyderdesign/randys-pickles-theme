  {{ '//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css' | stylesheet_tag }}
  <link href='//fonts.googleapis.com/css?family=Roboto+Condensed:400,700%7CRoboto:400,700%7CRoboto+Slab:400,700' rel='stylesheet' type='text/css'>
  
  {% if settings.google_header_font != "none" %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.google_header_font }}:300,400,700">
  {% endif %}
  {% if settings.google_nav_font != "none" %}
  <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family={{ settings.google_nav_font }}:300,400,700">
  {% endif %}
  
  <meta name="google-site-verification" content="vxwywUHXcSncpCn_A2OyHdLdL_QbJeDmCwTroue5SOI" />
  
</head>
<body>
  <!-- Begin toolbar -->
  <div class="toolbar-wrapper">
    <div class="toolbar clearfix">
      <div class="free-shipping-wrapper">
        <a href="/pages/shipping-returns" class="free-shipping-tag" onclick="ga('send', 'event', 'Universal Clicks', 'Header', 'Free Shipping');">
          <span class="fa fa-truck"></span>Free Shipping On All Orders*
        </a>
      </div>
      <ul class="unstyled">
        <li class="search-field fr">
          <form class="search" action="/search">
            <button type="submit" class="go"><i class="fa fa-search"></i></button>
            <input type="text" name="q" class="search_box" placeholder="Search" value="{{ search.terms }}" />
          </form>
        </li>
        {% if cart.item_count != 0 %}
        <!--li id="checkout-link" class="fr"><a href="/cart" class="btn" title="Checkout">Checkout</a></li-->
        {% endif %}
        <li class="fr">
          <a href="/cart" onclick="ga('send', 'event', 'Universal Clicks', 'Header', 'Cart');" class="cart-link" title="Shopping Cart"><i class="fa fa-shopping-cart"></i><strong>Pickle Shopping Cart</strong>( {{ cart.item_count }} )</a>
        </li>
        {% include 'top-nav' %}
        <li class="fr">
          <a href="/pages/where-to-buy-randys-pickles" onclick="ga('send', 'event', 'Universal Clicks', 'Header', 'Where To Buy');">Where To Buy</a>
        </li>          

        {% if settings.top_bar_linklist != blank %}
        {% for link in linklists[settings.top_bar_linklist].links %}
        <li class="fr">
          <a href="{{ link.url }}" title="{{ link.title }}">{{ link.title }}</a>
        </li>
        {% endfor %}
        {% endif %}
      </ul>
    </div>
  </div>
  <!-- End toolbar -->
  <!-- Begin Main Nav -->
  <div class="mainnav-wrapper">
    <!-- Begin right navigation -->
    {% if settings.navigation_alignment == "right" %}
    <div class="row mainnav clearfix" id="header">
      <div>
        <div class="inner-left logo-wrapper">
          <div class="logo">
            {% if settings.use_logo %}
            <a href="/" onclick="ga('send', 'event', 'Universal Clicks', 'Header', 'Logo');"><img src="{{ 'logo.png' | asset_url }}" alt="{{ shop.name }}" /></a>
            {% endif %}
            {% if template == 'index' %}
            <h1 style="display:none"><a href="/">{{ shop.name }}</a></h1>
            {% endif %}
          </div> 
        </div>        
        <a href="/cart" onclick="ga('send', 'event', 'Universal Clicks', 'Header', 'Mobile Cart');" class="mobile-cart" title="Shopping Cart"><i class="fa fa-shopping-cart"></i><strong>Cart</strong>( {{ cart.item_count }} )</a>
        <div id="menu-button" class="menu-icon">Menu<i class="fa fa-bars"></i></div>   
        <div id="nav" class="clearfix">
          <div class="inner-right fr">
            <nav class="main clearfix">
              {% include 'site-nav' %}
            </nav> 
          </div> 
        </div> 
      </div> 
    </div> 
    {% endif %}
    <!-- End right navigation -->
  </div>
  <!-- End Main Nav -->
  <!-- Begin Mobile Nav -->
  <div class="row mobile-wrapper">
    <nav class="mobile clearfix">
      {% include 'mobile-menu' %}
    </nav>
  </div>
  <!-- End Mobile Nav -->
  {% include 'countdown-clock' %}
  
  {% if template == 'index' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'homepage_video.jpg' | asset_url }}); background-position: top center;">
          <a href="#" onClick="revealModal(182708878); ga('send', 'event', 'Homepage Clicks', 'Slider', 'Sideburn Video'); return false;" class="video-slide">
          <div class="play-button">
            <span class="fa fa-play"></span>
          </div>
          </a>
        </li>
        <li style="background-image: url({{ 'homepage_TheLineUp.jpg' | asset_url }}); background-position: top center; display: none;">
          <div class="caption-wrapper">
            <div class="caption">
              <h2>Check Out the Line</h2>
              <h3>Find Your Flavor</h3>
              <a href="/collections/buy-pickles-online" onclick="ga('send', 'event', 'Homepage Clicks', 'Slider', 'Buy Pickles Online');" class="btn">Buy Pickles Online</a>
            </div>
          </div>
        </li>
      </ul>
      <div class="flex-controls clearfix"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  
  {% if page.url == '/pages/videos' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'homepage_video.jpg' | asset_url }}); background-position: top center;">
          <a href="#" onClick="revealModal(182708878); ga('send', 'event', 'Videos Clicks', 'Slider', 'Sideburns'); return false;" class="video-slide">
          <div class="play-button">
            <span class="fa fa-play"></span>
          </div>
          </a>
        </li>
        <li style="background-image: url({{ 'homepage_TheLineUp.jpg' | asset_url }}); background-position: top center; display: none;">
          <div class="caption-wrapper">
            <div class="caption">
              <h2>Check Out the Line</h2>
              <h3>Find Your Flavor</h3>
              <a href="/collections/buy-pickles-online" onclick="ga('send', 'event', 'Videos Clicks', 'Slider', 'Buy Pickles Online');" class="btn">Buy Pickles Online</a>
            </div>
          </div>
        </li>
      </ul>
      <div class="flex-controls clearfix"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  {% if page.url == '/pages/about-randys-pickles' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider about-us">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'about_1.jpg' | asset_url }});">
        </li>
      </ul>
      <div class="flex-controls clearfix hide"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  
  {% if page.url == '/pages/contact-us' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider about-us">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'homepage_newRetail.jpg' | asset_url }});">
        </li>
      </ul>
      <div class="flex-controls clearfix hide"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
    
  {% if page.url == '/apps/store-locator' || if page.url == '/pages/where-ro-buy' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider about-us">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'homepage-acme.jpg' | asset_url }});">
        </li>
      </ul>
      <div class="flex-controls clearfix hide"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  
  {% if page.handle == 'recipes' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider">
      <ul class="slides">
        <li style="background-image: url({{ 'homepage-recipe.jpg' | asset_url }}); background-position: top center; display: none;">
          <div class="caption-wrapper">
            <div class="caption">
              <h2>Win 2 Free Jars</h2>
              <h3>Recipe of the Month</h3>
              <a href="mailto: hello@randyspickles.com?subject=Recipe Contest" onclick="ga('send', 'event', 'Recipes Clicks', 'Body', 'Submit Recipe');" class="btn">Submit Your Recipe</a>
            </div>
          </div>
        </li>
      </ul>
      <div class="flex-controls clearfix"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  
  {% if page.url == '/pages/events' %}
  <!-- Begin slider -->
  <div class="flexslider-container">
    <div class="flexslider">
      <ul class="slides">
        <li class="lh0 bptc" style="background-image: url({{ 'events_1.jpg' | asset_url }});">
        </li>
        <li class="lh0 bptc" style="background-image: url({{ 'events_2.jpg' | asset_url }}); display: none;">
        </li>
      </ul>
      <div class="flex-controls clearfix"></div>
    </div>
  </div>
  <!-- End slider -->
  {% endif %}
  
  <!-- Vote Page -->
  {% if page.url == '/pages/vote' %}
    {% include 'vote' %}
  {% endif %}
  <!-- End Vote Page -->
  
  <!-- Begin wrapper -->
  <div id="transparency" class="wrapper">
    <div class="row">      
      <!-- Begin content-->
      <section id="content" class="clearfix">
        {{ content_for_layout }}
      </section>
      <!-- End content-->
    </div>
  </div>
  <!-- End wrapper -->
  <!-- Begin newsletter -->
  <div class="newsletter-wrapper">
    <div id="newsletter">
      <h3>Sign up for Randy's newsletter for savings on pickles, special seasonal pickles and pickled products, and big announcements!</h3>
      <br />
      <!-- Begin MailChimp Signup Form -->
      <div id="mc_embed_signup">
        <form action="//randyspickles.us7.list-manage.com/subscribe/post?u=5b880a33a965a7612a947364e&amp;id=09064490a8" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
          <div id="mc_embed_signup_scroll">
        <div class="mc-field-group">
          <!--label for="mce-EMAIL">Email Address  <span class="asterisk">*</span>
        </label-->
          <input type="email" value="" name="EMAIL" placeholder="Email Address" class="required email" id="mce-EMAIL">
          <input type="submit" value="Get Deals on Pickles" name="subscribe" id="mc-embedded-subscribe" class="btn newsletter">
        </div>
          <div id="mce-responses" class="clear">
            <div class="response" id="mce-error-response" style="display:none"></div>
            <div class="response" id="mce-success-response" style="display:none"></div>
          </div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
            <div style="position: absolute; left: -5000px;"><input type="text" name="b_5b880a33a965a7612a947364e_09064490a8" tabindex="-1" value=""></div>
          </div>
        </form>
      </div>
      <!-- End mc_embed_signup -->
      <!-- form action="{{ settings.mailing_list_form_action }}" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" target="_blank">
        <input type="email" value="" placeholder="Email Address" name="EMAIL" id="mail" />
        <input type="submit" class="btn newsletter" value="Sign Up" name="subscribe" id="subscribe" />
      </form -->
    </div>    
  </div>
  <!-- End newsletter -->
  <!-- Begin footer -->
  <div class="footer-wrapper">
    <footer>
      <div class="row">
        <!--div class="span12 full-border"></div-->
        {% assign blog_module_in_footer = false %}
        <!-- Begin latest blog post -->
        {% unless settings.footer_blog_post == blank or blogs[settings.footer_blog_post].empty? or blogs[settings.footer_blog_post].articles.size == 0 %}
        {% assign article = blogs[settings.footer_blog_post].articles.first %}
        {% assign blog_module_in_footer = true %}
        <!-- Not Used -->
        <div class="span4">
          <div class="p30">
            <h4><a href="/blogs/{{ settings.footer_blog_post }}">{{ settings.footer_blog_post_header }}</a></h4>
            <p class="p10"><strong>{{ article.title | link_to: article.url }}</strong></p>
            <p>{{ article.content | strip_html | truncatewords: 30 }}</p>
          </div>
        </div>
        {% endunless %}
        <!-- End latest blog post -->
        <!-- Begin footer navigation left -->
        <div class="span3 footer-menu">
          <!--h4>{{ settings.footer_linklist_title }}</h4-->
          <ul class="unstyled">
          {% for link in linklists.footer.links %}
            <li><a href="{{ link.url }}" onclick="ga('send', 'event', 'Universal Clicks', 'Footer', '{{ link.title }}');" title="{{ link.title }}">{{ link.title }}</a></li>
          {% endfor %}
          </ul>
        </div>
        <!-- End footer navigation left -->
        <!-- Begin footer navigation left -->
        <div class="span3 footer-menu right">
          <!--h4>{{ settings.footer_linklist_title }}</h4-->
          <ul class="unstyled">
          {% for link in linklists.footer-right.links %}
            <li><a href="{{ link.url }}" onclick="ga('send', 'event', 'Universal Clicks', 'Footer', '{{ link.title }}');" title="{{ link.title }}">{{ link.title }}</a></li>
          {% endfor %}
          </ul>
        </div>
        <!-- End footer navigation left -->
        {% unless blog_module_in_footer %}
        <div class="span6 tc">          
          <div class="clearfix">
            <a href="/"  onclick="ga('send', 'event', 'Universal Clicks', 'Footer', 'Logo - Footer');"><img class="footer-logo" src="{{ 'logo-footer-2x.png' | asset_url }}" alt="{{ shop.name }}" /></a>
            <p class="copyright">Copyright &copy; {{ 'now' | date: "%Y" }} {{ shop.name }}, LLC  </p>
          {% if settings.enable_social_links %}
            <!--h4>{{ settings.footer_social_title }}</h4-->
            {% include 'social-links' %}
          {% endif %}
          </div>
        </div>
        {% endunless %}
        <!-- Begin copyright -->
        <div class="span12 tc">
          {% if shop.enabled_payment_types.size > 0 %}
          <ul class="credit-cards clearfix">
            {% for type in shop.enabled_payment_types %}
            <li><img width="50" src="{{ type | payment_type_img_url }}" alt="credit card accepted" /></li>
            {% endfor %}
          </ul> <!-- /.credit-cards -->
          {% endif %}  
        </div>
        <!-- End copyright -->
      </div> 
    </footer>
  </div>
  <!-- End footer -->
  
  {% comment %}
  {{ 'html5shiv.js' | asset_url | script_tag }}
  {% endcomment %}
  {{ content_for_header }}
  <!--script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7/jquery.min.js"></script-->  
  
  {{ 'jquery.min.js' | asset_url | script_tag }}
  <script type="text/javascript" async src="{{ 'option_selection.js' | shopify_asset_url }}"></script>
  <script type="text/javascript" async src="{{ 'api.jquery.js' | shopify_asset_url }}"></script>
  {% if template contains 'customers' %}
  <script type="text/javascript" async src="{{ 'shopify_common.js' | shopify_asset_url }}"></script>
  <script type="text/javascript" async src="{{ 'customer_area.js' | shopify_asset_url }}"></script>
  {% endif %}
  {% if settings.display_slideshow or if page.url == '/pages/videos' %}
  <script type="text/javascript">
