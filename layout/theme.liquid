  {% if page.url == '/pages/vote' %}
    {{ 'jquery.ajaxchimp.js' | asset_url | script_tag }}
    <script type="text/javascript">
    function callbackFunction (resp) {
        if (resp.result === 'success') {
            alert('Thanks! Make sure you confirm your vote in the email we just sent or you won\'t be entered to win!');
        }
        else {
          alert('Opps! It looks like you already voted. Only one vote per email!');
        }
    }
    
    $('#mc-form').ajaxChimp({
        url: 'https://randyspickles.us7.list-manage.com/subscribe/post?u=5b880a33a965a7612a947364e&amp;id=fa022c6532',
        callback: callbackFunction
    });
    
    $('#vote-slider input[type=radio]').change(function() {
      $('#vote-slider .info').removeClass('selected');
      $(this).parent('.info').addClass('selected');
    });
    
    $('.vote-button').click(function(e) {
      if (!$('.vote-email').val()) {
        e.preventDefault();
        alert('You need to fill in your email!');
        return false;
      }
      if($('.vote-container input:radio:checked').length == 0) {
        e.preventDefault();
        alert('You need to select your favorite flavor!');
        return false;
      }
    });
    
      
      (function() {

        // store the slider in a local variable
        var $window = $(window),
            flexslider = { vars:{} };

        // tiny helper function to add breakpoints
        function getGridSize() {
          return (window.innerWidth < 500) ? 1 :
                 (window.innerWidth < 800) ? 2 :
                 (window.innerWidth < 900) ? 3 : 3;
        }

        $window.load(function() {
          $('#vote-slider').flexslider({
            animation: "slide",
            animationSpeed: 400,
            animationLoop: false,
            itemWidth: 300,
            itemMargin: 0,
            move: 1,
            minItems: getGridSize(), // use function to pull in initial value
            maxItems: getGridSize(), // use function to pull in initial value
            start: function(slider){
              $('body').removeClass('loading');
              flexslider = slider;
            }
          });
        });

        // check grid size on resize event
        $window.resize(function() {
          var gridSize = getGridSize();

          flexslider.vars.minItems = gridSize;
          flexslider.vars.maxItems = gridSize;
        });
      }());
     
    </script>
  {% endif %}
